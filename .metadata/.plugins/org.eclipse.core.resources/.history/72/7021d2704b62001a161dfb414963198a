package personal;

public class Socio extends Persona{
	
	private final NumeroSocio numeroSocio;
	private CuotaSocio cuota;
	private boolean activo;

	public Socio(String nombre, String sexo, int edad) {
		super(nombre, sexo, edad);
		numeroSocio = new NumeroSocio();
		cuota = new CuotaSocio();
		activo = true;
	}

	public NumeroSocio getNumeroSocio() {
		return numeroSocio;
	}

	public CuotaSocio getCuota() {
		return cuota;
	}

	public boolean isActivo() {
		return activo;
	}

	public void setActivo(boolean activo) {
		this.activo = activo;
	}

	
}
